<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <configSections>
    </configSections>
    <connectionStrings>
        <add name="Toyota.eCRB.iCROP.DataAccess.SC3180201.My.MySettings.ConnectionString"
            connectionString="Data Source=tact;User ID=watool;Password=1234;Unicode=True"
            providerName="System.Data.OracleClient" />
        <add name="Toyota.eCRB.iCROP.DataAccess.SC3180201.My.MySettings.Toyota_eCRBConnectionString"
            connectionString="Data Source=.;Initial Catalog=Toyota_eCRB;User ID=sa;Password=1"
