        <add name="Toyota.eCRB.ServerCheck.CheckResult.DataAccess.SC3180201.My.MySettings.ConnectionString"
            connectionString="Data Source=tact;User ID=watool;Password=1234;Unicode=True"
            providerName="System.Data.OracleClient" />
        <add name="Toyota.eCRB.ServerCheck.CheckResult.DataAccess.SC3180201.My.MySettings.Toyota_eCRBConnectionString"
