        <Item Name="isReplaceAuthority" SerializeAs="System.Boolean" Value="false"/>
        <!-- isReplaceAuthority = true の場合に　Authority部（Scheam, host, port） を置換する文字列
              http://i-crop.gtmc.com.cn と設定されている場合
              https://192.168.10.1:8100/iCROP/SC3010101.aspx という url は
              http://i-crop.gtmc.com.cn/iCROP/SC3010101.aspx に置換される
        -->
        <Item Name="ReplaceAuthority" SerializeAs="System.String" Value="http://i-crop.gtmc.com.cn"/>
