    <!--OpenAM認証-->
    <Class Name="OpenAMSettings">
      <Setting Name="">
        <!-- グローバルSSO を利用するか否か -->
        <Item Name="GSSOEnabled" SerializeAs="System.Boolean" Value="false" />
        <!-- Idp が SSO Token を格納するCookie名 -->
        <Item Name="IdpCookie" SerializeAs="System.String" Value="iPlanetDirectoryPro" />
        <!-- ログインページの URL -->
        <Item Name="IdpLoginPageUrl" SerializeAs="System.String" Value="http://sso.gtmc.com.cn/SSOAuth/UI/Login" />
        <!-- IdpCookieに格納されている SSO Token が認証状態か確認するための Idp の URL -->
        <Item Name="IsSsoTokenValidUrl" SerializeAs="System.String" Value="http://sso.gtmc.com.cn/openam/identity/isTokenValid" />
        <!-- 認証されているユーザの属性をを要求する Idp の URL -->
        <Item Name="IdpAttributeRequestUrl" SerializeAs="System.String" Value="http://sso.gtmc.com.cn/openam/identity/attributes" />
        <!-- SLO(Single Log Out) を要求する Idp の URL -->
        <Item Name="IdpSloUrl" SerializeAs="System.String" Value="http://sso.gtmc.com.cn/openam/identity/logout" />
        <Item Name="DecryptWebService" SerializeAs="System.String" Value="http://sso.gtmc.com.cn/openam/services/DecryptWebservice" />


        <!-- システム：ユーザの属性データ取得のためのキーのマッピング -->
